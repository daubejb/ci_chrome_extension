{
    "manifest_version": 2,
    "name": "Corporate Information Lookup",
    "version": "1.0",

    "description": "Lookup Corporate Information",

    "content_scripts": [ {
        "all_frames": true,
        "js": [ "item.js" ],
        "matches": [ "http://*/*", "https://*/*" ]
    } ],
    "permissions": [
        "activeTab"
    ]
}
